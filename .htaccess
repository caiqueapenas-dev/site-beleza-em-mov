# Redireciona apenas a raiz do dom√≠nio para /loja
RewriteEngine On
RewriteCond %{HTTP_HOST} ^(www\.)?belezaemov\.com$ [NC]
RewriteRule ^$ /loja/ [L,R=301]

<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
